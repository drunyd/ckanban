{
  "manifest_version": 3,
  "name": "CKanban",
  "version": "0.2.0",
  "description": "Multi-project Kanban board (Backlog, In Progress, On Hold, Complete).",
  "chrome_url_overrides": { "newtab": "ui/board.html" },
  "icons": {
    "16": "assets/icons/icon16.png",
    "48": "assets/icons/icon48.png",
    "128": "assets/icons/icon128.png"
  },
  "permissions": ["storage"],
  "host_permissions": []
}